<script>
	    // 获取List
	function openNewWindow(id) {
		let a = $("<a href='detailed.html?id=" + id+"' target='_blank'></a>").get(0);
		let e = document.createEvent('MouseEvents');
		e.initEvent('click', true, true);
		a.dispatchEvent(e);
	}
</script>